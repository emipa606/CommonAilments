<Defs>
  <HediffDef ParentName="InjuryBase" Name="CA_Knick" Abstract="true">
    <label>knick</label>
    <labelNoun>a knick</labelNoun>
    <description>A knick</description>
    <hediffClass>HediffWithComps</hediffClass>
    <defaultLabelColor>(0.32,0.7,0.32)</defaultLabelColor>
    <scenarioCanAdd>false</scenarioCanAdd>
    <maxSeverity>1.0</maxSeverity>
    <initialSeverity>1.0</initialSeverity>
    <isBad>false</isBad>
    <tendable>true</tendable>
    <comps>
      <li Class="HediffCompProperties_TendDuration">
        <baseTendDurationHours>36</baseTendDurationHours>
        <severityPerDayTended>-0.75</severityPerDayTended>
      </li>
      <li Class="HediffCompProperties_Immunizable">
        <severityPerDayNotImmune>0.00</severityPerDayNotImmune>
      </li>
      <li Class="ComAil.HediffCompProperties_CACure">
        <CureHoursMin>1</CureHoursMin>
        <CureHoursMax>2</CureHoursMax>
      </li>
    </comps>
  </HediffDef>
